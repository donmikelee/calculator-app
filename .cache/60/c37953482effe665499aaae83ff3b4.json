}],"sources":{"src\\styles\\styles.scss":"@import 'themes/theme-1.scss';\r\n\r\nhtml,\r\nbody {\r\n  height: 100%;\r\n  width: 100%;\r\n}\r\n\r\n*,\r\n*::after,\r\n*::before {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n  font-family: 'Spartan', sans-serif;\r\n  transition: background-color 500ms ease-in-out, color 500ms ease-in-out;\r\n  display: flex;\r\n\r\n  &.dark {\r\n    color: $text-white;\r\n    background-color: $main-background;\r\n  }\r\n\r\n  &.light {\r\n    color: $text-gray;\r\n    background-color: $main-background-light;\r\n  }\r\n\r\n  &.custom {\r\n    color: $text-yellow;\r\n    background-color: $main-background-custom;\r\n  }\r\n}\r\n\r\n#calc-app {\r\n  width: 540px;\r\n  max-height: 708px;\r\n  margin: auto;\r\n\r\n  .calc-header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    height: 44px;\r\n    margin-bottom: 32px;\r\n  }\r\n\r\n  .calc-theme {\r\n    display: flex;\r\n\r\n    h2 {\r\n      margin-right: 21px;\r\n      margin-top: 31px;\r\n      font-size: 12px;\r\n      text-transform: uppercase;\r\n    }\r\n\r\n    .toggler {\r\n      width: 71px;\r\n\r\n      .labels {\r\n        ul {\r\n          display: flex;\r\n          list-style-type: none;\r\n          justify-content: center;\r\n          padding-left: 5px;\r\n\r\n          li:nth-last-child(2) {\r\n            margin-left: 13px;\r\n            margin-right: 13px;\r\n          }\r\n        }\r\n      }\r\n      .switcher {\r\n        display: flex;\r\n        justify-content: space-evenly;\r\n        align-items: center;\r\n        margin-top: 5px;\r\n        border-radius: 13px;\r\n        height: 26px;\r\n        position: relative;\r\n        transition: background-color 500ms ease-in-out;\r\n\r\n        &.dark {\r\n          background-color: $keypad-toggle-background;\r\n        }\r\n\r\n        &.light {\r\n          background-color: $keypad-toggle-background-light;\r\n        }\r\n\r\n        &.custom {\r\n          background-color: $keypad-toggle-background-custom;\r\n        }\r\n\r\n        input {\r\n          z-index: 3;\r\n          cursor: pointer;\r\n          opacity: 0;\r\n        }\r\n\r\n        span {\r\n          height: 16px;\r\n          width: 16px;\r\n          border-radius: 50%;\r\n          position: absolute;\r\n          z-index: 2;\r\n          transition: all 0.3s ease-in-out;\r\n          left: 6px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .toggler .switcher input[value='dark']:checked ~ span {\r\n    left: 6px;\r\n    background-color: $key-red;\r\n  }\r\n  .toggler .switcher input[value='light']:checked ~ span {\r\n    left: 27px;\r\n    background-color: $key-background-light;\r\n  }\r\n  .toggler .switcher input[value='custom']:checked ~ span {\r\n    left: 49px;\r\n    background-color: $key-custom;\r\n  }\r\n\r\n  .calc-display {\r\n    min-height: 128px;\r\n    border-radius: 10px;\r\n    margin-bottom: 24px;\r\n    transition: background-color 500ms ease-in-out;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    font-size: 48px;\r\n    font-weight: 600;\r\n    align-items: center;\r\n\r\n    &.dark {\r\n      background-color: $screen-background;\r\n    }\r\n\r\n    &.light {\r\n      background-color: $screen-background-light;\r\n    }\r\n\r\n    &.custom {\r\n      background-color: $screen-background-custom;\r\n    }\r\n\r\n    .current-number {\r\n      width: 100%;\r\n      text-align: end;\r\n      padding-right: 32px;\r\n      height: 56px;\r\n    }\r\n    .previous-number {\r\n      width: 100%;\r\n      font-size: 22px;\r\n      text-align: start;\r\n      padding-left: 32px;\r\n      height: 26px;\r\n    }\r\n  }\r\n\r\n  .calc-keyboard {\r\n    height: 480px;\r\n    border-radius: 10px;\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    padding: 32px 32px 32px 32px;\r\n    justify-content: space-evenly;\r\n    align-items: center;\r\n    font-size: 32px;\r\n    font-weight: 600;\r\n    transition: background-color 500ms ease-in-out, color 500ms ease-in-out;\r\n\r\n    &.dark {\r\n      background-color: $keypad-toggle-background;\r\n      color: $text-blue;\r\n    }\r\n\r\n    &.light {\r\n      background-color: $keypad-toggle-background-light;\r\n      color: $text-gray;\r\n    }\r\n\r\n    &.custom {\r\n      background-color: $keypad-toggle-background-custom;\r\n      color: $text-yellow;\r\n    }\r\n\r\n    .calc-button {\r\n      border: none;\r\n      height: 64px;\r\n      width: 101px;\r\n      border-radius: 10px;\r\n      margin-right: 22px;\r\n      margin-bottom: 24px;\r\n      text-transform: uppercase;\r\n      cursor: pointer;\r\n      display: flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n      opacity: 0.9;\r\n      transition: background-color 500ms ease-in-out;\r\n      box-shadow: 0px 6px 0px 0px $key-shadow-two;\r\n\r\n      &:hover {\r\n        opacity: 1;\r\n      }\r\n\r\n      &.dark {\r\n        background-color: $key-background-two;\r\n      }\r\n\r\n      &.light {\r\n        background-color: $key-background-two-light;\r\n      }\r\n\r\n      &.custom {\r\n        background-color: $key-background-custom;\r\n        box-shadow: 0px 6px 0px 0px $key-shadow-two-custom;\r\n      }\r\n    }\r\n\r\n    .calc-button:nth-child(4),\r\n    .calc-button:nth-child(17) {\r\n      font-size: 20px;\r\n      transition: background-color 500ms ease-in-out, color 500ms ease-in-out;\r\n      box-shadow: 0px 6px 0px 0px $key-shadow;\r\n\r\n      &.dark {\r\n        background-color: $key-background;\r\n        color: $text-white;\r\n      }\r\n\r\n      &.light {\r\n        background-color: $key-background-light;\r\n        color: $text-white;\r\n      }\r\n\r\n      &.custom {\r\n        background-color: $key-background-custom;\r\n        box-shadow: 0px 6px 0px 0px $key-shadow-two-custom;\r\n      }\r\n    }\r\n\r\n    .calc-button:nth-child(4),\r\n    .calc-button:nth-child(8),\r\n    .calc-button:nth-child(12),\r\n    .calc-button:nth-child(16) {\r\n      margin-right: 0;\r\n    }\r\n\r\n    .calc-button:nth-child(17),\r\n    .calc-button:nth-child(18) {\r\n      width: 227px;\r\n      margin-bottom: 0;\r\n    }\r\n\r\n    .calc-button:nth-child(18) {\r\n      margin-right: 0;\r\n      transition: background-color 500ms ease-in-out, color 500ms ease-in-out, box-shadow 500ms ease-in-out;\r\n      box-shadow: 0px 6px 0px 0px $key-dark-red;\r\n\r\n      &.dark {\r\n        background-color: $key-red;\r\n        color: $text-white;\r\n      }\r\n\r\n      &.light {\r\n        background-color: $key-light;\r\n        color: $text-white;\r\n        box-shadow: 0px 6px 0px 0px $key-brown-light;\r\n      }\r\n\r\n      &.custom {\r\n        background-color: $key-custom;\r\n        color: $text-dark-blue;\r\n        box-shadow: 0px 6px 0px 0px $key-dark-custom;\r\n      }\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"ade6f1165aada10983133151d80d6417","cacheData":{"env":{}}}